# Load Balancer Observability Dashboard - Environment Configuration Template
# Copy this file to .env and configure with your actual database credentials

# SQL Server Database Configuration
DB_SERVER=YOUR_SERVER_NAME
DB_DATABASE=TrafficInsights
DB_AUTH_TYPE=Windows Authentication
DB_USERNAME=YOUR_DOMAIN\YOUR_USERNAME
DB_PASSWORD=
DB_DRIVER=ODBC Driver 17 for SQL Server

# Optional: Connection timeout settings
DB_CONNECTION_TIMEOUT=30
DB_COMMAND_TIMEOUT=30

# Application Settings
LOG_LEVEL=INFO
DATA_DIRECTORY=../data/
